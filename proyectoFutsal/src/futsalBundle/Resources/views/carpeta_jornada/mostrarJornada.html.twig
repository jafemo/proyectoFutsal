{% extends '::plantilla.html.twig' %}
{% block body %}
<ul>
{% for jornada in TablaJornada %}

<h2>{{jornada.numero}}</h2>
<p>{{jornada.equipo}}</p>
<p>{{jornada.rival}}</p>
<a href="{{ path('borrarJornada', {'id': jornada.id}) }}">Borrar jornada</a>
<br>
<a href="{{ path('editarJornada', {'id': jornada.id}) }}">Editar jornada</a>
{% endfor %}
</ul>
{% endblock %}
